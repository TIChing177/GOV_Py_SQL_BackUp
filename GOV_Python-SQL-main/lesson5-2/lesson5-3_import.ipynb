    "conn = psycopg2.connect(\"\")\n",
    "cur = conn.cursor()\n",
    "sql = '''\n",
    "    create table if not exists student_score(\n",
    "\tid serial primary key,\n",
    "\tname varchar(5),\n",
    "\tchinese smallint,\n",
    "\tmath smallint,\n",
    "\tenglish smallint);\n",
    "    '''\n",
    "cur.execute(sql)\n",
    "conn.commit()\n",
    "cur.close()\n",
    "conn.close()  "
   ]
